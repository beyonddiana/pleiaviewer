<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater can_close="true" can_drag_on_left="false" can_minimize="true" rect_control="FloaterSimExport"
     can_resize="true" min_width="340" width="340" height="300" name="SimExport" title="Export Simulator">

 <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
     bottom="-40" drop_shadow_visible="true" follows="left|top"
     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="10"
     mouse_opaque="true" name="simprims" v_pad="0" width="166">Simulator prims: 0</text>
 
<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
     bottom_delta="-20" drop_shadow_visible="true" follows="left|top"
     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="10"
     mouse_opaque="true" name="volumes" v_pad="0" width="166">Cached volumes : 0</text>
<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
     bottom_delta="-20" drop_shadow_visible="true" follows="left|top"
     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="10"
     mouse_opaque="true" name="plants" v_pad="0" width="166">Cached plants : 0</text>

<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
     bottom_delta="-20" drop_shadow_visible="true" follows="left|top"
     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="10"
     mouse_opaque="true" name="missing" v_pad="0" width="166">Missing : 0</text>

  
<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
     bottom_delta="-30" drop_shadow_visible="true" follows="left|top"
     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="10"
     mouse_opaque="true" name="export_label" v_pad="0" width="166">Export:</text>

  <check_box bottom_delta="-25" follows="left|top"
     font="SansSerifSmall" height="16" initial_value="true" 
     label="PNG Textures" left="10" name="export_textures"
     width="200" />
  <check_box bottom_delta="0" follows="left|top"
     font="SansSerifSmall" height="16" initial_value="true"
     label="TGA Textures" left="130" name="export_textures_tga"
     width="200" />

  <check_box bottom_delta="-25" follows="left|top"
     font="SansSerifSmall" height="16" initial_value="true" 
     label="Plants" left="10" name="export_plants"
     width="200" />

  <check_box bottom_delta="-25" follows="left|top"
      font="SansSerifSmall" height="16" initial_value="true"
      label="Object Properties" left="10" name="export_properties"
      width="200" />

  <check_box bottom_delta="0" follows="left|top"
	   font="SansSerifSmall" height="16" initial_value="true"
	   label="Object Inventory" left="130" name="export_contents"
	   width="200" />

  <check_box bottom_delta="-25" follows="left|top"
      font="SansSerifSmall" height="16" initial_value="true"
      label="Move avatar" left="10" name="move"
      width="200" />
  
  <spinner bottom_delta="-20" decimal_digits="0" follows="left|top" height="16"
       increment="5.0" initial_val="40" label="Maximum Altitude" label_width="100" left="10"
       max_val="4096" min_val="20" mouse_opaque="true" name="altitude"
       text_enabled_color="110, 15, 15, 255" width="156" />

  <spinner bottom_delta="-20" decimal_digits="0" follows="left|top" height="16"
       increment="5.0" initial_val="15" label="Start export when missing prims less than " label_width="220" left="10"
       max_val="4096" min_val="0" mouse_opaque="true" name="object_threshold"
       text_enabled_color="110, 15, 15, 255" width="280" />

  <spinner bottom_delta="-20" decimal_digits="0" follows="left|top" height="16"
       increment="1.0" initial_val="3" label="and hasn't decreased in " label_width="130" left="10"
       max_val="4096" min_val="1" mouse_opaque="true" name="time_threshold"
       text_enabled_color="110, 15, 15, 255" width="180" />

  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
       bottom_delta="0" drop_shadow_visible="true" follows="left|top"
       font="SansSerifSmall" h_pad="0" halign="left" height="16" left="200"
       mouse_opaque="true" name="export_label" v_pad="0" width="166">minutes</text>
  
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
       bottom_delta="-20" drop_shadow_visible="true" follows="left|top"
       font="SansSerifSmall" h_pad="0" halign="left" height="16" left="10"
       mouse_opaque="true" name="timelast" v_pad="0" width="200">0 seconds since last new object received</text>

  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
       bottom_delta="-20" drop_shadow_visible="true" follows="left|top"
       font="SansSerifSmall" h_pad="0" halign="left" height="16" left="10"
       mouse_opaque="true" name="curpos" v_pad="0" width="166">x,y,z</text>

  <button bottom="5" follows="right|bottom" font="SansSerif" halign="center" height="20"
     label="Backup simulator" label_selected="" right="-200"
     mouse_opaque="true" name="export"
     tool_tip="Export the selected linksets" width="130" />

  <button bottom="5" follows="right|bottom" font="SansSerif" halign="center" height="20"
     label="Save terrain" label_selected="" right="-70"
     mouse_opaque="true" name="saveterrain" enabled="false"
     tool_tip="Save terrain" width="130" />

  <button bottom="5" follows="right|bottom" font="SansSerif" halign="center" height="20"
     label="Close" label_selected="" right="-10"
     mouse_opaque="true" name="close" width="60" />
</floater>
